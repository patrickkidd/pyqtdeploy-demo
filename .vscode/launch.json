    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "--env": {
                "PYTHONPATH": "${workspaceFolder}/site-packages"
            }
        },
        {
            "name": "pyqtdeploy-sysroot",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/bin/pyqtdeploy-sysroot-debuggable.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [],
            "--env": {
                "PYTHONPATH": "${workspaceFolder}/site-packages"
            }
        }
    ]
}